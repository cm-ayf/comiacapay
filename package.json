      "@wyw-in-js/shared": "^0.8.0",
      "@wyw-in-js/transform": "^0.8.0"
    },
    "@pigment-css/vite-plugin": {
      "@wyw-in-js/shared": "^0.8.0",
      "@wyw-in-js/transform": "^0.8.0"
    },
    "vite": "$vite"
