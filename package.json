{
  "name": "comiacapay",
  "version": "0.3.0",
  "private": true,
  "type": "module",
  "sideEffects": false,
  "scripts": {
    "postinstall": "run-p postinstall:*",
    "postinstall:patch-package": "patch-package",
    "postinstall:prisma-generate": "prisma generate",
    "postinstall:react-router-typegen": "react-router typegen",
    "prepare": "node .husky/install.js",
    "build": "react-router build",
    "preview": "vite preview",
    "dev": "react-router dev",
    "check:tsgo": "tsgo",
    "check:oxlint": "oxlint --type-aware .",
    "check:oxfmt": "oxfmt --check .",
    "check": "run-p check:*",
    "fix:oxlint": "oxlint --fix",
    "fix:oxfmt": "oxfmt --write .",
    "fix": "run-s fix:*",
    "migrate:dev": "prisma migrate dev",
    "migrate:deploy": "prisma migrate deploy",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug"
  },
  "dependencies": {
    "@conform-to/react": "1.17.1",
    "@conform-to/valibot": "1.17.1",
    "@mui/icons-material": "7.3.7",
    "@mui/lab": "7.0.1-beta.21",
    "@mui/material": "7.3.7",
    "@mui/material-pigment-css": "7.3.7",
    "@mui/system": "7.3.7",
    "@mui/utils": "7.3.7",
    "@mui/x-charts": "8.27.0",
    "@mui/x-data-grid": "8.27.0",
    "@pigment-css/react": "0.0.30",
    "@prisma/adapter-pg": "6.19.0",
    "@prisma/client": "6.19.0",
    "@react-router/fs-routes": "7.13.0",
    "@react-router/node": "7.13.0",
    "discord-api-types": "0.38.38",
    "idb": "^8.0.3",
    "isbot": "5.1.34",
    "patch-package": "8.0.1",
    "react": "19.2.4",
    "react-dom": "19.2.4",
    "react-router": "7.13.0",
    "valibot": "1.2.0"
  },
  "devDependencies": {
    "@oxlint/plugins": "1.43.0",
    "@pigment-css/vite-plugin": "0.0.30",
    "@playwright/test": "1.58.2",
    "@prisma/generator-helper": "6.19.0",
    "@react-router/dev": "7.13.0",
    "@remix-run/node-fetch-server": "0.13.0",
    "@tsconfig/node24": "24.0.4",
    "@tsconfig/strictest": "2.0.8",
    "@types/node": "24.10.4",
    "@types/pg": "8.16.0",
    "@types/react": "19.2.13",
    "@types/react-dom": "19.2.3",
    "@types/serviceworker": "0.0.184",
    "@typescript/native-preview": "7.0.0-dev.20260210.1",
    "@vercel/react-router": "1.2.5",
    "dotenv": "^17.2.4",
    "eslint-plugin-mui-path-imports": "0.0.15",
    "eslint-plugin-mui-pigment-css": "./eslint-plugin-mui-pigment-css",
    "husky": "9.1.7",
    "lint-staged": "16.2.7",
    "npm-run-all2": "8.0.4",
    "oxfmt": "0.31.0",
    "oxlint": "1.46.0",
    "oxlint-tsgolint": "0.12.0",
    "prisma": "6.19.0",
    "prisma-json-types-generator": "3.6.2",
    "vite": "8.0.0-beta.13",
    "vite-plugin-pwa": "1.2.0"
  },
  "overrides": {
    "@pigment-css/react": {
      "@mui/system": "$@mui/system",
      "@mui/utils": "$@mui/utils",
