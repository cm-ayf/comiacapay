{
  "name": "comiacapay",
  "version": "0.3.0",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "postinstall": "prisma generate",
    "prepare": "node .husky/install.js",
    "build": "remix vite:build",
    "dev": "remix vite:dev",
    "check:prettier": "prettier --check .",
    "check:eslint": "eslint --cache --cache-location ./node_modules/.cache/eslint .",
    "check:tsc": "tsc",
    "check": "run-p check:*",
    "fix:prettier": "prettier --write .",
    "fix:eslint": "eslint --cache --cache-location ./node_modules/.cache/eslint --fix .",
    "fix": "run-s fix:*",
    "start": "remix-serve ./build/server/index.js",
    "typecheck": "tsc"
  },
  "dependencies": {
    "@mui/material": "6.1.3",
    "@mui/material-pigment-css": "6.1.3",
    "@prisma/client": "5.20.0",
    "@remix-pwa/worker-runtime": "2.1.4",
    "@remix-run/node": "^2.12.0",
    "@remix-run/react": "2.12.0",
    "@remix-run/serve": "2.12.0",
    "isbot": "4.1.0",
    "jose": "5.9.3",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "typescript-eslint": "8.8.1"
  },
  "devDependencies": {
    "@eslint/compat": "^1.2.0",
    "@eslint/js": "^9.12.0",
    "@pigment-css/vite-plugin": "0.0.24",
    "@remix-pwa/dev": "3.1.0",
    "@remix-run/dev": "2.12.0",
    "@tsconfig/node20": "20.1.4",
    "@tsconfig/remix": "^2.0.5",
    "@tsconfig/strictest": "2.0.5",
    "@types/node": "20.16.11",
    "@types/react": "18.3.11",
    "@types/react-dom": "18.3.0",
    "@types/serviceworker": "0.0.98",
    "@typescript-eslint/eslint-plugin": "8.8.1",
    "@typescript-eslint/parser": "8.8.1",
    "@vercel/remix": "2.12.0",
    "eslint": "9.12.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-import-resolver-typescript": "3.6.3",
    "eslint-plugin-import-x": "4.3.1",
    "eslint-plugin-jsx-a11y": "6.10.0",
    "eslint-plugin-mui-path-imports": "^0.0.15",
    "eslint-plugin-react": "7.37.1",
    "eslint-plugin-react-hooks": "4.6.2",
    "globals": "15.11.0",
    "husky": "9.1.6",
    "lint-staged": "15.2.10",
    "npm-run-all2": "6.2.3",
    "prettier": "3.3.3",
    "prisma": "5.20.0",
    "prisma-json-types-generator": "3.0.4",
    "typescript": "5.5.4",
    "vite": "5.4.8",
    "vite-tsconfig-paths": "5.0.1"
  },
  "overrides": {
    "eslint-plugin-react-hooks": {
      "eslint": "^9"
    }
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
